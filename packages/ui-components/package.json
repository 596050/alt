{
	"name": "@packages/ui-components",
	"version": "1.0.0",
	"packageManager": "yarn@3.3.0",
	"main": "./dist/index.js",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"require": "./dist/index.js",
			"import": "./dist/index.mjs",
			"types": "./dist/index.d.ts"
		}
	},
	"scripts": {
		"package:build": "NODE_ENV=production webpack",
		"build:prod": "npx tsup src/index.ts --format cjs,esm --dts --treeshake --clean --external react --silent",
		"build:dev": "npx --yes tsup src/index.ts --format cjs,esm --treeshake --external react --sourcemap inline --watch",
		"build": "tsup --config ./tsup.config.ts",
		"clean": "rm -rf dist && rm -rf node_modules && rm -rf .turbo",
		"gen:svgs": "npx @svgr/cli -d src/assets/components src/assets/svg/ --typescript"
	},
	"dependencies": {
		"@emotion/css": "^11.10.5",
		"@emotion/react": "^11.10.5",
		"@emotion/styled": "^11.10.5",
		"@mui/icons-material": "^5.11.0",
		"@mui/lab": "^5.0.0-alpha.117",
		"@mui/material": "^5.11.6",
		"@mui/styles": "^5.11.2",
		"@mui/x-data-grid-pro": "^5.17.21",
		"feather-icons": "^4.29.0",
		"@react-hook/resize-observer": "^1.2.6",
		"debounce-promise": "^3.1.2",
		"@tippyjs/react": "^4.2.6",
		"classnames": "^2.3.2",
		"country-list": "^2.3.0",
		"date-fns": "^2.29.3",
		"final-form": "^4.20.9",
		"final-form-arrays": "^3.1.0",
		"flag": "^5.0.1",
		"guid-typescript": "^1.0.9",
		"js-cookie": "^3.0.1",
		"libphonenumber-js": "^1.10.19",
		"lodash": "^4.17.21",
		"match-sorter": "^6.3.1",
		"polished": "^4.2.2",
		"react": "^18.*",
		"react-final-form": "^6.5.9",
		"react-final-form-arrays": "^3.1.4",
		"react-final-form-listeners": "^1.0.3",
		"react-focus-lock": "^2.9.3",
		"react-helmet": "^6.1.0",
		"react-intersection-observer": "^9.4.1",
		"react-intl": "^6.2.6",
		"react-number-format": "^5.1.3",
		"react-router-dom": "^6.8.0",
		"react-router-navigation-prompt": "^1.9.6",
		"react-select": "^5.7.0",
		"react-textarea-autosize": "^8.4.0",
		"styled-components": "^5.3.6",
		"use-local-storage": "^2.3.6",
		"uuid": "^9.0.0",
		"validator": "^13.7.0",
		"yup": "^0.32.11"
	},
	"devDependencies": {
		"@types/classnames": "^2.3.1",
		"@types/debounce-promise": "^3.1.6",
		"@types/react": "^18.*",
		"@types/styled-components": "^5.1.26",
		"@types/validator": "^13.7.11",
		"tsup": "^6.5.0",
		"typescript": "^5.*",
		"typescript-plugin-css-modules": "^4.1.1"
	}
}
